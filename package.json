{
  "name": "dsa-ts",
  "version": "0.0.0-development",
  "description": "A collection of data structures and algorithms in Typescript",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest --config jest.config.js --maxWorkers=1",
    "semantic-release": "semantic-release",
    "commit": "npx git-cz",
    "report-coverage": "codecov"
  },
  "repository": "https://github.com/jeffzh4ng/dsa-ts",
  "author": "Jeff Zhang",
  "license": "MIT",
  "prepublish": "tsc",
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "codecov": "3.8.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.23.0",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "semantic-release": "17.4.2",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "pre-push": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*": [
      "yarn prettier --write '**/*.ts'"
    ]
  }
}
